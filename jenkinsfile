pipeline{
    
    agent any 
    
    stages {
        
        stage('Git Checkout'){
            
            steps{
                
                script{
                  git branch: 'main', url: 'https://github.com/adammheni/pipeline.git'

        }
                    }          
                     }
                    stage('Quality Gate Status'){
                
                steps{
                    
                    script{
                        
              waitForQualityGate abortPipeline: false, credentialsId: 'sonar-key-2'
                    }
                }
            }
                     }
                     }


